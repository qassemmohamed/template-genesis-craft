# Ignore all node_modules folders
**/node_modules/

# Environment variable files
.env
.env.*  # For things like .env.local, .env.production

# Log files
npm-debug.log*
yarn-debug.log*
yarn-error.log*
pnpm-debug.log*

# Dependency directories
bower_components/

# Build output
dist/
build/
out/

# OS-specific files
.DS_Store
Thumbs.db

# IDEs and editors
.vscode/
.idea/
*.sublime-workspace
*.sublime-project

# Optional runtime files
*.pid
*.seed
*.pid.lock

# Coverage reports
coverage/
.nyc_output/

# Next.js / Nuxt / Vite / Astro build cache
.next/
.nuxt/
.vite/
.cache/
.svelte-kit/

# TypeScript cache
*.tsbuildinfo

# Parcel cache
.cache/

# Temporary files
tmp/
temp/
*.swp

# Mac system files
.AppleDouble
.LSOverride

# Git folder
.git/

# Ignore build artifacts
*.tgz
*.zip

# Firebase / Supabase / similar CLI outputs
.firebase/
.supabase/

